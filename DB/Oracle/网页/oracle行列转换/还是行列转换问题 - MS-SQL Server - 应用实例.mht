	{
		var ss =3D this.selection.start;
		var se =3D this.selection.end;
		var st =3D this.selection.top;
		if (ss <=3D se)
		{
			var s =3D this.editor.value;
			var left =3D s.substring(0, ss);
			var center =3D s.substring(ss, se);
			var right =3D s.substring(se, s.length);
